 "raw_base": {"__comment": "DF Raw information for plant types, if adding a new type must add here, seeds from plant type overwrite seeds from plant growths",
     "TREE": {"__comment": "TREE type is anything that produces WOOD",
         "BASE":      ["[NAME:#NAME_SINGULAR][NAME_PLURAL:#NAME_PLURAL][ADJ:#NAME_ADJ]",
                       "[VALUE:2]","[FREQUENCY:50]","[SAPLING]",
                       "#SEASONS","#PREFSTRING"],
         "BIOMES":    ["#ENVIRONMENT","#BIOME"],
         "STRUCTURE_MAT": ["[USE_MATERIAL_TEMPLATE:STRUCTURAL:STRUCTURAL_PLANT_TEMPLATE]"],
         "STRUCTURE": ["[USE_ MATERIAL_TEMPLATE:WOOD:WOOD_TEMPLATE]",
                       "[TREE:LOCAL_PLANT_MAT:WOOD]",
                       "\t[TREE_COLOR:#COLOR_BASE][SAPLING_COLOR:#COLOR_BASE]",
                       "\t[TREE_TILE:226]",
                       "\t[TRUNK_PERIOD:10]",
                       "\t[HEAVY_BRANCH_DENSITY:25]",
                       "\t[BRANCH_DENSITY:50]",
                       "\t[MAX_TRUNK_HEIGHT:5]",
                       "\t[HEAVY_BRANCH_RADIUS:1]",
                       "\t[BRANCH_RADIUS:2]",
                       "\t[TRUNK_BRANCHING:2]",
                       "\t[MAX_TRUNK_DIAMETER:1]",
                       "\t[TRUNK_WIDTH_PERIOD:200]",
                       "\t[ROOT_DENSITY:5]",
                       "\t[ROOT_RADIUS:3]",
                       "\t[STANDARD_TILE_NAMES]",
                       "[BASIC_MAT:LOCAL_PLANT_MAT:STRUCTURAL]"],
         "SEED_MAT":  ["[USE_MATERIAL_TEMPLATE:SEED:SEED_TEMPLATE]"],
         "SEED":      ["[SEED:#NAME_SEED:#COLOR_SEED:LOCAL_PLANT_MAT:SEED]"]
     },
     "GRASS": {"__comment": "GRASS type is NOT DF GRASS type",
         "BASE":      ["[NAME:#NAME_SINGULAR][NAME_PLURAL:#NAME_PLURAL][ADJ:#NAME_ADJ]",
                       "[PICKED_TILE:58][PICKED_COLOR:#COLOR_BASE][SHRUB_COLOR:#COLOR_BASE]",
                       "[VALUE:2]","[FREQUENCY:50]","[CLUSTERSIZE:5]",
                       "#SEASONS","#PREFSTRING"],
         "BIOMES":    ["#ENVIRONMENT","#BIOME"],
         "STRUCTURE_MAT": ["[USE_MATERIAL_TEMPLATE:STRUCTURAL:STRUCTURAL_PLANT_TEMPLATE]"],
         "STRUCTURE": ["[BASIC_MAT:LOCAL_PLANT_MAT:STRUCTURAL]"],
         "SEED_MAT":  ["[USE_MATERIAL_TEMPLATE:SEED:SEED_TEMPLATE]"],
         "SEED":      ["[SEED:#NAME_SEED:#COLOR_SEED:LOCAL_PLANT_MAT:SEED]"]
     },
